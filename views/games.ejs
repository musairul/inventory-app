<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="styles.css" />
    <title>Inventory</title>
  </head>
  <body>
    <%-include("./partials/header")%>
    <main>
      <h1>All Games</h1>
      <a href="/addGame"><button class="newGame-btn">Add New</button></a>
      <div class="container">
        <% games.forEach(game => { %> <%-include("./partials/card",
        {title:game.title,developers:game.developers,genres:game.genres,
        id:game.id})%> <% }) %>
      </div>
    </main>
  </body>
</html>
